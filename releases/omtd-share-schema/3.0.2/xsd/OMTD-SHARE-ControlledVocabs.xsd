						<subclassOf>http://w3id.org/meta-share/omtd-share/StructuralAnnotationType</subclassOf>
					</xs:appinfo>
				</xs:annotation>
			</xs:enumeration>
			<xs:enumeration value="http://w3id.org/meta-share/omtd-share/Citation">
				<xs:annotation>
