	</xs:simpleType>
	<xs:simpleType name="annotationTypeType">
		<xs:annotation>
			<xs:documentation>Specifies the annotation type of the resource or what a s/w component consumes or produces as an output</xs:documentation>
			<xs:appinfo>
				<label>Annotation type</label>
				<label-plural>Annotation types</label-plural>
			</xs:appinfo>
		</xs:annotation>
		<xs:restriction base="xs:anyURI">
			<xs:enumeration value="http://w3id.org/meta-share/omtd-share/DocumentAnnotationType">
				<xs:annotation>
					<xs:documentation>Any kind of annotation that is used to describe a document (e.g. identifier, size, location, language etc.)</xs:documentation>
					<xs:appinfo>
						<label>Document annotation type</label>
						<subclassOf>http://w3id.org/meta-share/omtd-share/AnnotationType</subclassOf>
						<altLabel>Document information</altLabel>
						<altLabel>Document metadata</altLabel>
					</xs:appinfo>
				</xs:annotation>
			</xs:enumeration>
			<xs:enumeration value="http://w3id.org/meta-share/omtd-share/PartOfSpeech">
				<xs:annotation>
					<xs:documentation>A division of words based on common grammatical features</xs:documentation>
					<xs:appinfo>
						<label>Part of Speech</label>
						<subclassOf>http://w3id.org/meta-share/omtd-share/AnnotationType</subclassOf>
						<altLabel>Morphosyntactic category</altLabel>
						<altLabel>Word category</altLabel>
						<altLabel>Grammatical category</altLabel>
					</xs:appinfo>
				</xs:annotation>
			</xs:enumeration>
			<xs:enumeration value="http://w3id.org/meta-share/omtd-share/Domain-specificAnnotation">
				<xs:annotation>
					<xs:documentation>Any kind of annotation that is used for specific domains (e.g. genes and proteins from the biomedical domain, plants from agriculture etc.)</xs:documentation>
					<xs:appinfo>
						<label>Domain-specific annotation type</label>
						<subclassOf>http://w3id.org/meta-share/omtd-share/AnnotationType</subclassOf>
					</xs:appinfo>
				</xs:annotation>
			</xs:enumeration>
			<xs:enumeration value="http://w3id.org/meta-share/omtd-share/SocialSciencesEntity">
				<xs:annotation>
					<xs:documentation>Any kind of annotation that pertains to entities of social sciences; the use of TheSoz is recommended</xs:documentation>
					<xs:appinfo>
						<label>Social sciences entity</label>
						<subclassOf>http://w3id.org/meta-share/omtd-share/Domain-specificAnnotation</subclassOf>
					</xs:appinfo>
				</xs:annotation>
			</xs:enumeration>
			<xs:enumeration value="http://w3id.org/meta-share/omtd-share/AllbusVariable">
				<xs:annotation>
					<xs:documentation>ALLBUS variable</xs:documentation>
					<xs:appinfo>
						<label>ALLBUS variable</label>
						<subclassOf>http://w3id.org/meta-share/omtd-share/SocialSciencesEntity</subclassOf>
					</xs:appinfo>
				</xs:annotation>
			</xs:enumeration>
			<xs:enumeration value="http://w3id.org/meta-share/omtd-share/HistoricalEvent">
				<xs:annotation>
					<xs:documentation>Historical event</xs:documentation>
					<xs:appinfo>
						<label>Historical event</label>
						<subclassOf>http://w3id.org/meta-share/omtd-share/SocialSciencesEntity</subclassOf>
						<subclassOf>http://w3id.org/meta-share/omtd-share/NamedEntity</subclassOf>
					</xs:appinfo>
				</xs:annotation>
			</xs:enumeration>
			<xs:enumeration value="http://w3id.org/meta-share/omtd-share/MethodOfResearch">
				<xs:annotation>
					<xs:documentation>Method of research</xs:documentation>
					<xs:appinfo>
						<label>Method of research</label>
						<subclassOf>http://w3id.org/meta-share/omtd-share/NamedEntity</subclassOf>
						<subclassOf>http://w3id.org/meta-share/omtd-share/SocialSciencesEntity</subclassOf>
					</xs:appinfo>
				</xs:annotation>
			</xs:enumeration>
			<xs:enumeration value="http://w3id.org/meta-share/omtd-share/Media">
				<xs:annotation>
					<xs:documentation>The main means of mass communication (broadcasting, publishing, and the Internet) regarded collectively [https://en.oxforddictionaries.com/definition/media]</xs:documentation>
					<xs:appinfo>
						<label>Media</label>
						<subclassOf>http://w3id.org/meta-share/omtd-share/NamedEntity</subclassOf>
						<subclassOf>http://w3id.org/meta-share/omtd-share/SocialSciencesEntity</subclassOf>
					</xs:appinfo>
				</xs:annotation>
			</xs:enumeration>
			<xs:enumeration value="http://w3id.org/meta-share/omtd-share/TheoreticalFrame">
				<xs:annotation>
					<xs:documentation>Theoretical frame</xs:documentation>
					<xs:appinfo>
						<label>Theoretical frame</label>
						<subclassOf>http://w3id.org/meta-share/omtd-share/SocialSciencesEntity</subclassOf>
						<subclassOf>http://w3id.org/meta-share/omtd-share/NamedEntity</subclassOf>
					</xs:appinfo>
				</xs:annotation>
			</xs:enumeration>
			<xs:enumeration value="http://w3id.org/meta-share/omtd-share/OfficialText">
				<xs:annotation>
					<xs:documentation>Official text</xs:documentation>
					<xs:appinfo>
						<label>Official text</label>
						<subclassOf>http://w3id.org/meta-share/omtd-share/NamedEntity</subclassOf>
						<subclassOf>http://w3id.org/meta-share/omtd-share/SocialSciencesEntity</subclassOf>
					</xs:appinfo>
				</xs:annotation>
			</xs:enumeration>
			<xs:enumeration value="http://w3id.org/meta-share/omtd-share/LinguisticEntity">
				<xs:annotation>
					<xs:documentation>Any kind of annotation pertaining to entities of linguistics; the use of OLIA is recommended</xs:documentation>
					<xs:appinfo>
						<label>Linguistic entity</label>
						<subclassOf>http://w3id.org/meta-share/omtd-share/Domain-specificAnnotation</subclassOf>
					</xs:appinfo>
				</xs:annotation>
			</xs:enumeration>
			<xs:enumeration value="http://w3id.org/meta-share/omtd-share/Relation">
				<xs:annotation>
					<xs:documentation>Any type of relation that holds between two or more entities of a specific domain</xs:documentation>
					<xs:appinfo>
						<label>Relation</label>
						<subclassOf>http://w3id.org/meta-share/omtd-share/Domain-specificAnnotation</subclassOf>
					</xs:appinfo>
				</xs:annotation>
			</xs:enumeration>
			<xs:enumeration value="http://w3id.org/meta-share/omtd-share/AgriculturalEntity">
				<xs:annotation>
					<xs:documentation>Any kind of annotation pertaining to entities of the agricultural domain; the use of the AGROVOC thesaurus is recommended</xs:documentation>
					<xs:appinfo>
						<label>Agricultural entity</label>
						<subclassOf>http://w3id.org/meta-share/omtd-share/Domain-specificAnnotation</subclassOf>
					</xs:appinfo>
				</xs:annotation>
			</xs:enumeration>
			<xs:enumeration value="http://w3id.org/meta-share/omtd-share/Phenotype">
				<xs:annotation>
					<xs:documentation>The physical appearance or biochemical characteristic of an organism as a result of the interaction of its genotype and the environment [http://www.biology-online.org/dictionary/Phenotype]</xs:documentation>
					<xs:appinfo>
						<label>Phenotype</label>
						<subclassOf>http://w3id.org/meta-share/omtd-share/AgriculturalEntity</subclassOf>
					</xs:appinfo>
				</xs:annotation>
			</xs:enumeration>
			<xs:enumeration value="http://w3id.org/meta-share/omtd-share/PhysicoChemicalProperty">
				<xs:annotation>
					<xs:documentation>Physical and chemical property of substances</xs:documentation>
					<xs:appinfo>
						<label>Physico-chemical property</label>
						<subclassOf>http://w3id.org/meta-share/omtd-share/AgriculturalEntity</subclassOf>
					</xs:appinfo>
				</xs:annotation>
			</xs:enumeration>
			<xs:enumeration value="http://w3id.org/meta-share/omtd-share/ProteinFamily">
				<xs:annotation>
					<xs:documentation>A protein family is a group of proteins that share a common evolutionary origin, reflected by their related functions and similarities in sequence or structure [https://www.ebi.ac.uk/training/online/course/introduction-protein-classification-ebi/protein-classification/what-are-protein-families]</xs:documentation>
					<xs:appinfo>
						<label>Protein family</label>
