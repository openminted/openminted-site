						<label>RDF formats</label>
						<subclassOf>http://w3id.org/meta-share/omtd-share/DataFormat</subclassOf>
					</xs:appinfo>
				</xs:annotation>
			</xs:enumeration>
			<xs:enumeration value="http://w3id.org/meta-share/omtd-share/Obo">
				<xs:annotation>
					<xs:documentation>Serialization format for ontologies according to the Open Biomedical Ontologies model.</xs:documentation>
					<xs:appinfo>
						<label>OBO</label>
						<subclassOf>http://w3id.org/meta-share/omtd-share/RdfFormats</subclassOf>
					</xs:appinfo>
				</xs:annotation>
			</xs:enumeration>
			<xs:enumeration value="http://w3id.org/meta-share/omtd-share/Owl">
				<xs:annotation>
					<xs:documentation>Superclass for formats used for OWL</xs:documentation>
					<xs:appinfo>
						<label>OWL</label>
						<subclassOf>http://w3id.org/meta-share/omtd-share/RdfFormats</subclassOf>
					</xs:appinfo>
				</xs:annotation>
			</xs:enumeration>
			<xs:enumeration value="http://w3id.org/meta-share/omtd-share/Owl_xml">
				<xs:annotation>
					<xs:documentation>XML format for OWL ontologies</xs:documentation>
					<xs:appinfo>
						<label>OWL/XML</label>
						<subclassOf>http://w3id.org/meta-share/omtd-share/Xml</subclassOf>
						<subclassOf>http://w3id.org/meta-share/omtd-share/Owl</subclassOf>
					</xs:appinfo>
				</xs:annotation>
			</xs:enumeration>
			<xs:enumeration value="http://w3id.org/meta-share/omtd-share/Rdf_xml">
