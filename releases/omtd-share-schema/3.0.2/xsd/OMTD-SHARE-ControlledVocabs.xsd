<?xml version="1.0" encoding="UTF-8"?>
<xs:schema xmlns:ms="http://www.meta-share.org/OMTD-SHARE_XMLSchema" xmlns:xs="http://www.w3.org/2001/XMLSchema" targetNamespace="http://www.meta-share.org/OMTD-SHARE_XMLSchema" elementFormDefault="qualified" attributeFormDefault="unqualified" version="3.0.2" xml:lang="en">
	<xs:simpleType name="applicationType">
		<xs:annotation>
			<xs:documentation>A classification of NLP/TDM applications; please, note that the ontology is under revision</xs:documentation>
			<xs:appinfo>
				<label>Application type</label>
				<label-plural>Application types</label-plural>
			</xs:appinfo>
		</xs:annotation>
		<xs:restriction base="xs:anyURI">
			<xs:enumeration value="annotation">
				<xs:annotation>
					<xs:appinfo>
						<label>annotation</label>
					</xs:appinfo>
				</xs:annotation>
			</xs:enumeration>
		</xs:restriction>
	</xs:simpleType>
	<xs:simpleType name="operationType">
		<xs:annotation>
			<xs:documentation>Specifies the function/task the component/application performs</xs:documentation>
			<xs:appinfo>
				<relation>one-to-many</relation>
				<label>Type of operation</label>
				<label-plural>Types of operation</label-plural>
			</xs:appinfo>
		</xs:annotation>
		<xs:restriction base="xs:anyURI">
			<xs:enumeration value="http://w3id.org/meta-share/omtd-share/SupportComponent">
				<xs:annotation>
					<xs:documentation>A component that provides support to developers</xs:documentation>
					<xs:appinfo>
						<label>Support component</label>
						<subclassOf>http://w3id.org/meta-share/omtd-share/ComponentType</subclassOf>
						<altLabel>Helper</altLabel>
					</xs:appinfo>
				</xs:annotation>
			</xs:enumeration>
			<xs:enumeration value="http://w3id.org/meta-share/omtd-share/Validator">
				<xs:annotation>
					<xs:documentation>A component used to confirm that a system/resource meets the specifications and fulfills its intended purpose</xs:documentation>
					<xs:appinfo>
						<label>Validator</label>
						<subclassOf>http://w3id.org/meta-share/omtd-share/SupportComponent</subclassOf>
					</xs:appinfo>
				</xs:annotation>
			</xs:enumeration>
			<xs:enumeration value="http://w3id.org/meta-share/omtd-share/CrowdsourcingComponent">
				<xs:annotation>
					<xs:documentation>A component that supports crowdsourcing operations</xs:documentation>
					<xs:appinfo>
						<label>Crowdsourcing component</label>
						<subclassOf>http://w3id.org/meta-share/omtd-share/SupportComponent</subclassOf>
					</xs:appinfo>
				</xs:annotation>
			</xs:enumeration>
			<xs:enumeration value="http://w3id.org/meta-share/omtd-share/DataCollector">
				<xs:annotation>
					<xs:documentation>A component that collects (retrieves) data from various sources</xs:documentation>
					<xs:appinfo>
						<label>Data collector</label>
						<subclassOf>http://w3id.org/meta-share/omtd-share/SupportComponent</subclassOf>
					</xs:appinfo>
				</xs:annotation>
			</xs:enumeration>
			<xs:enumeration value="http://w3id.org/meta-share/omtd-share/Crawler">
				<xs:annotation>
					<xs:documentation>A component that crawls the web and collects data from various web sites</xs:documentation>
					<xs:appinfo>
						<label>Crawler</label>
						<subclassOf>http://w3id.org/meta-share/omtd-share/DataCollector</subclassOf>
					</xs:appinfo>
				</xs:annotation>
			</xs:enumeration>
			<xs:enumeration value="http://w3id.org/meta-share/omtd-share/MachineLearningPredictor">
				<xs:annotation>
					<xs:documentation>A component that is used in predicting based on machine learning models</xs:documentation>
					<xs:appinfo>
						<label>Machine Learning predictor</label>
						<subclassOf>http://w3id.org/meta-share/omtd-share/SupportComponent</subclassOf>
					</xs:appinfo>
				</xs:annotation>
			</xs:enumeration>
			<xs:enumeration value="http://w3id.org/meta-share/omtd-share/FeatureExtractor">
				<xs:annotation>
					<xs:documentation>A component that is used for extracting features</xs:documentation>
					<xs:appinfo>
						<label>Feature extractor</label>
						<subclassOf>http://w3id.org/meta-share/omtd-share/SupportComponent</subclassOf>
					</xs:appinfo>
				</xs:annotation>
			</xs:enumeration>
			<xs:enumeration value="http://w3id.org/meta-share/omtd-share/Visualiser">
				<xs:annotation>
					<xs:documentation>A component or interface that renders the contents of a resource in a graphic way for visualisation purposes</xs:documentation>
					<xs:appinfo>
						<label>Visualiser</label>
						<subclassOf>http://w3id.org/meta-share/omtd-share/SupportComponent</subclassOf>
					</xs:appinfo>
				</xs:annotation>
			</xs:enumeration>
			<xs:enumeration value="http://w3id.org/meta-share/omtd-share/DataMerger">
				<xs:annotation>
					<xs:documentation>A component that supports data merging from various sources</xs:documentation>
					<xs:appinfo>
						<label>Data merger</label>
						<subclassOf>http://w3id.org/meta-share/omtd-share/SupportComponent</subclassOf>
					</xs:appinfo>
				</xs:annotation>
			</xs:enumeration>
			<xs:enumeration value="http://w3id.org/meta-share/omtd-share/Evaluator">
				<xs:annotation>
					<xs:documentation>A component that is used in the evaluation of the performance of a component</xs:documentation>
					<xs:appinfo>
						<label>Evaluator</label>
						<subclassOf>http://w3id.org/meta-share/omtd-share/SupportComponent</subclassOf>
					</xs:appinfo>
				</xs:annotation>
			</xs:enumeration>
			<xs:enumeration value="http://w3id.org/meta-share/omtd-share/TrainerOfMachineLearningModels">
				<xs:annotation>
					<xs:documentation>A component that is used in training models for machine learning</xs:documentation>
					<xs:appinfo>
						<label>Trainer of Machine Learning models</label>
						<subclassOf>http://w3id.org/meta-share/omtd-share/SupportComponent</subclassOf>
						<altLabel>ML trainer</altLabel>
						<altLabel>ML models trainer</altLabel>
					</xs:appinfo>
				</xs:annotation>
			</xs:enumeration>
			<xs:enumeration value="http://w3id.org/meta-share/omtd-share/Debugger">
				<xs:annotation>
					<xs:documentation>A component that is used in the debugging process</xs:documentation>
					<xs:appinfo>
						<label>Debugger</label>
						<subclassOf>http://w3id.org/meta-share/omtd-share/SupportComponent</subclassOf>
					</xs:appinfo>
				</xs:annotation>
			</xs:enumeration>
			<xs:enumeration value="http://w3id.org/meta-share/omtd-share/Editor">
				<xs:annotation>
					<xs:documentation>A component that allows humans to edit the contents of a resource</xs:documentation>
					<xs:appinfo>
						<label>Editor</label>
						<subclassOf>http://w3id.org/meta-share/omtd-share/SupportComponent</subclassOf>
					</xs:appinfo>
				</xs:annotation>
			</xs:enumeration>
			<xs:enumeration value="http://w3id.org/meta-share/omtd-share/SoftwareDevelopmentEnvironment">
				<xs:annotation>
					<xs:documentation>A technology that supports the development of software components and data resources required for their operation</xs:documentation>
					<xs:appinfo>
						<label>Software development environment</label>
						<subclassOf>http://w3id.org/meta-share/omtd-share/SupportComponent</subclassOf>
					</xs:appinfo>
				</xs:annotation>
			</xs:enumeration>
			<xs:enumeration value="http://w3id.org/meta-share/omtd-share/FlowController">
				<xs:annotation>
					<xs:documentation>A component that supports controlling flows</xs:documentation>
					<xs:appinfo>
						<label>Flow controller</label>
						<subclassOf>http://w3id.org/meta-share/omtd-share/SupportComponent</subclassOf>
					</xs:appinfo>
				</xs:annotation>
			</xs:enumeration>
			<xs:enumeration value="http://w3id.org/meta-share/omtd-share/DataSplitter">
				<xs:annotation>
					<xs:documentation>A component that performs data splitting for cross validation purposes</xs:documentation>
					<xs:appinfo>
						<label>Data splitter</label>
						<subclassOf>http://w3id.org/meta-share/omtd-share/SupportComponent</subclassOf>
					</xs:appinfo>
				</xs:annotation>
			</xs:enumeration>
			<xs:enumeration value="http://w3id.org/meta-share/omtd-share/ScriptBasedAnalyser">
				<xs:annotation>
					<xs:documentation>A component that performs analysis tasks based on a script</xs:documentation>
					<xs:appinfo>
						<label>Script-based analyser</label>
						<subclassOf>http://w3id.org/meta-share/omtd-share/SupportComponent</subclassOf>
					</xs:appinfo>
				</xs:annotation>
			</xs:enumeration>
			<xs:enumeration value="http://w3id.org/meta-share/omtd-share/Viewer">
				<xs:annotation>
					<xs:documentation>A component that supports humans in accessing the contents of a resource</xs:documentation>
					<xs:appinfo>
						<label>Viewer</label>
						<subclassOf>http://w3id.org/meta-share/omtd-share/SupportComponent</subclassOf>
					</xs:appinfo>
				</xs:annotation>
			</xs:enumeration>
			<xs:enumeration value="http://w3id.org/meta-share/omtd-share/CorpusViewer">
				<xs:annotation>
					<xs:documentation>A component that supports humans in accessing the contents of a corpus</xs:documentation>
					<xs:appinfo>
						<label>Corpus viewer</label>
						<subclassOf>http://w3id.org/meta-share/omtd-share/Viewer</subclassOf>
					</xs:appinfo>
				</xs:annotation>
			</xs:enumeration>
			<xs:enumeration value="http://w3id.org/meta-share/omtd-share/LexiconViewer">
				<xs:annotation>
					<xs:documentation>A component that supports humans in accessing the contents of a lexical/conceptual resource</xs:documentation>
					<xs:appinfo>
						<label>Lexicon viewer</label>
						<subclassOf>http://w3id.org/meta-share/omtd-share/Viewer</subclassOf>
					</xs:appinfo>
				</xs:annotation>
			</xs:enumeration>
			<xs:enumeration value="http://w3id.org/meta-share/omtd-share/Converter">
				<xs:annotation>
					<xs:documentation>A component that performs conversion between formats of a resource</xs:documentation>
					<xs:appinfo>
						<label>Converter</label>
						<subclassOf>http://w3id.org/meta-share/omtd-share/SupportComponent</subclassOf>
					</xs:appinfo>
				</xs:annotation>
			</xs:enumeration>
			<xs:enumeration value="http://w3id.org/meta-share/omtd-share/DependencyConverter">
				<xs:annotation>
					<xs:documentation>A component that converts a constituency tree into a dependency tree</xs:documentation>
					<xs:appinfo>
						<label>Dependency converter</label>
						<subclassOf>http://w3id.org/meta-share/omtd-share/Converter</subclassOf>
					</xs:appinfo>
				</xs:annotation>
			</xs:enumeration>
			<xs:enumeration value="http://w3id.org/meta-share/omtd-share/Matcher">
				<xs:annotation>
					<xs:documentation>A component that allows matching of elements</xs:documentation>
					<xs:appinfo>
						<label>Matcher</label>
						<subclassOf>http://w3id.org/meta-share/omtd-share/SupportComponent</subclassOf>
					</xs:appinfo>
				</xs:annotation>
			</xs:enumeration>
			<xs:enumeration value="http://w3id.org/meta-share/omtd-share/GazeteerBasedMatcher">
				<xs:annotation>
					<xs:documentation>A component that allows matching of elements based on a gazeteer</xs:documentation>
					<xs:appinfo>
						<label>Gazeteer based matcher</label>
						<subclassOf>http://w3id.org/meta-share/omtd-share/Matcher</subclassOf>
					</xs:appinfo>
				</xs:annotation>
			</xs:enumeration>
			<xs:enumeration value="http://w3id.org/meta-share/omtd-share/AccessComponent">
				<xs:annotation>
					<xs:documentation>A component that provides access to data resources, e.g. reads a resource or writes the output of a process in a certain format</xs:documentation>
					<xs:appinfo>
						<label>Access Component</label>
						<subclassOf>http://w3id.org/meta-share/omtd-share/ComponentType</subclassOf>
					</xs:appinfo>
				</xs:annotation>
			</xs:enumeration>
			<xs:enumeration value="http://w3id.org/meta-share/omtd-share/Reader">
				<xs:annotation>
					<xs:documentation>A component that reads content of various types (pdf, txt, xml etc.)</xs:documentation>
					<xs:appinfo>
						<label>Reader</label>
						<subclassOf>http://w3id.org/meta-share/omtd-share/AccessComponent</subclassOf>
					</xs:appinfo>
				</xs:annotation>
			</xs:enumeration>
			<xs:enumeration value="http://w3id.org/meta-share/omtd-share/Writer">
				<xs:annotation>
					<xs:documentation>A component that writes processing results in various formats</xs:documentation>
					<xs:appinfo>
						<label>Writer</label>
						<subclassOf>http://w3id.org/meta-share/omtd-share/AccessComponent</subclassOf>
					</xs:appinfo>
				</xs:annotation>
			</xs:enumeration>
			<xs:enumeration value="http://w3id.org/meta-share/omtd-share/Processor">
				<xs:annotation>
					<xs:documentation>A component that is used in processing operations</xs:documentation>
					<xs:appinfo>
						<label>Processor</label>
						<subclassOf>http://w3id.org/meta-share/omtd-share/ComponentType</subclassOf>
					</xs:appinfo>
				</xs:annotation>
			</xs:enumeration>
			<xs:enumeration value="http://w3id.org/meta-share/omtd-share/Annotator">
				<xs:annotation>
					<xs:documentation>A component that annotates any data (text, video, audio etc.), i.e. adds any descriptive or analytic notations (structural, linguistic, etc) to raw data</xs:documentation>
					<xs:appinfo>
						<label>Annotator</label>
						<subclassOf>http://w3id.org/meta-share/omtd-share/Processor</subclassOf>
						<altLabel>Tagger</altLabel>
					</xs:appinfo>
				</xs:annotation>
			</xs:enumeration>
			<xs:enumeration value="http://w3id.org/meta-share/omtd-share/CoReferenceAnnotator">
				<xs:annotation>
					<xs:documentation>A component that annotates tokens of a text with coreference labels, marking expressions that refer to the same entity in the text</xs:documentation>
					<xs:appinfo>
						<label>Co-reference annotator</label>
						<subclassOf>http://w3id.org/meta-share/omtd-share/Annotator</subclassOf>
					</xs:appinfo>
				</xs:annotation>
			</xs:enumeration>
			<xs:enumeration value="http://w3id.org/meta-share/omtd-share/Lemmatizer">
				<xs:annotation>
					<xs:documentation>A component that annotates the tokens of a text with lemma information</xs:documentation>
					<xs:appinfo>
						<label>Lemmatizer</label>
						<subclassOf>http://w3id.org/meta-share/omtd-share/Annotator</subclassOf>
					</xs:appinfo>
				</xs:annotation>
			</xs:enumeration>
			<xs:enumeration value="http://w3id.org/meta-share/omtd-share/Stemmer">
				<xs:annotation>
					<xs:documentation>A component that extracts stems from words in a text, usually by removing the most common morphological and inflectional endings from words</xs:documentation>
					<xs:appinfo>
						<label>Stemmer</label>
						<subclassOf>http://w3id.org/meta-share/omtd-share/Annotator</subclassOf>
					</xs:appinfo>
				</xs:annotation>
			</xs:enumeration>
			<xs:enumeration value="http://w3id.org/meta-share/omtd-share/Parser">
				<xs:annotation>
					<xs:documentation>A component that takes as input text and returns a form of data structure (e.g. syntactic parse as a tree, or bracketed structure etc.)</xs:documentation>
					<xs:appinfo>
						<label>Parser</label>
						<subclassOf>http://w3id.org/meta-share/omtd-share/Annotator</subclassOf>
						<altLabel>Syntactic analyzer</altLabel>
					</xs:appinfo>
				</xs:annotation>
			</xs:enumeration>
			<xs:enumeration value="http://w3id.org/meta-share/omtd-share/DependencyParser">
				<xs:annotation>
					<xs:documentation>A component that generates a dependency tree from typically token and part-of-speech annotations</xs:documentation>
					<xs:appinfo>
						<label>Dependency parser</label>
						<subclassOf>http://w3id.org/meta-share/omtd-share/Parser</subclassOf>
					</xs:appinfo>
				</xs:annotation>
			</xs:enumeration>
			<xs:enumeration value="http://w3id.org/meta-share/omtd-share/ConstituencyParser">
				<xs:annotation>
					<xs:documentation>A component that builds a constituency tree from typically token and part-of-speech annotations</xs:documentation>
					<xs:appinfo>
						<label>Constituency parser</label>
						<subclassOf>http://w3id.org/meta-share/omtd-share/Parser</subclassOf>
					</xs:appinfo>
				</xs:annotation>
			</xs:enumeration>
			<xs:enumeration value="http://w3id.org/meta-share/omtd-share/NamedEntitityRecognizer">
				<xs:annotation>
					<xs:documentation>A component that seeks to locate and classify elements in a text into pre-defined categories such as the names of persons, organizations, locations, expressions of times, discipline-specific classes, etc</xs:documentation>
					<xs:appinfo>
						<label>Named entitity recognizer</label>
						<subclassOf>http://w3id.org/meta-share/omtd-share/Annotator</subclassOf>
					</xs:appinfo>
				</xs:annotation>
			</xs:enumeration>
			<xs:enumeration value="http://w3id.org/meta-share/omtd-share/MorphologicalTagger">
				<xs:annotation>
					<xs:documentation>A component that annotates tokens of a text with morphological information (part-of-speech and morphological features)</xs:documentation>
					<xs:appinfo>
						<label>Morphological tagger</label>
						<subclassOf>http://w3id.org/meta-share/omtd-share/Annotator</subclassOf>
					</xs:appinfo>
				</xs:annotation>
			</xs:enumeration>
			<xs:enumeration value="http://w3id.org/meta-share/omtd-share/PartOfSpeechTagger">
				<xs:annotation>
					<xs:documentation>A component that annotates tokens of a text with part-of-speech information</xs:documentation>
					<xs:appinfo>
						<label>Part of speech tagger</label>
						<subclassOf>http://w3id.org/meta-share/omtd-share/MorphologicalTagger</subclassOf>
						<altLabel>PoS tagger</altLabel>
					</xs:appinfo>
				</xs:annotation>
			</xs:enumeration>
			<xs:enumeration value="http://w3id.org/meta-share/omtd-share/SemanticAnnotator">
				<xs:annotation>
					<xs:documentation>A component that annotates the tokens of a text with semantic features</xs:documentation>
					<xs:appinfo>
						<label>Semantic annotator</label>
						<subclassOf>http://w3id.org/meta-share/omtd-share/Annotator</subclassOf>
					</xs:appinfo>
				</xs:annotation>
			</xs:enumeration>
			<xs:enumeration value="http://w3id.org/meta-share/omtd-share/AnnotatorOfSemanticRoleLabels">
				<xs:annotation>
					<xs:documentation>A component that annotates the tokens of a text with Semantic Role labels</xs:documentation>
					<xs:appinfo>
						<label>Annotator of semantic role labels</label>
						<subclassOf>http://w3id.org/meta-share/omtd-share/SemanticAnnotator</subclassOf>
					</xs:appinfo>
				</xs:annotation>
			</xs:enumeration>
			<xs:enumeration value="http://w3id.org/meta-share/omtd-share/ReadabilityAnnotator">
				<xs:annotation>
					<xs:documentation>A component that annotates the tokens of a text with readability scores</xs:documentation>
					<xs:appinfo>
						<label>Readability annotator</label>
						<subclassOf>http://w3id.org/meta-share/omtd-share/SemanticAnnotator</subclassOf>
					</xs:appinfo>
				</xs:annotation>
			</xs:enumeration>
			<xs:enumeration value="http://w3id.org/meta-share/omtd-share/Aligner">
				<xs:annotation>
					<xs:documentation>A component that detects and annotates equivalence relations between items (corpora, texts, paragraphs, sentences, phrases, words) in two languages</xs:documentation>
					<xs:appinfo>
						<label>Aligner</label>
						<subclassOf>http://w3id.org/meta-share/omtd-share/Annotator</subclassOf>
					</xs:appinfo>
				</xs:annotation>
			</xs:enumeration>
			<xs:enumeration value="http://w3id.org/meta-share/omtd-share/Chunker">
				<xs:annotation>
					<xs:documentation>A component that groups tokens of a text into chunks</xs:documentation>
					<xs:appinfo>
						<label>Chunker</label>
						<subclassOf>http://w3id.org/meta-share/omtd-share/Annotator</subclassOf>
					</xs:appinfo>
				</xs:annotation>
			</xs:enumeration>
			<xs:enumeration value="http://w3id.org/meta-share/omtd-share/Segmenter">
				<xs:annotation>
					<xs:documentation>A component that segments a text into structural untis (chapters, paragraphs, sentences, words, tokens etc.)</xs:documentation>
					<xs:appinfo>
						<label>Segmenter</label>
						<subclassOf>http://w3id.org/meta-share/omtd-share/Annotator</subclassOf>
					</xs:appinfo>
				</xs:annotation>
			</xs:enumeration>
			<xs:enumeration value="http://w3id.org/meta-share/omtd-share/Tokenizer">
				<xs:annotation>
					<xs:documentation>A component that recognizes and tags tokens (words, punctuation marks, digits etc.) in a text</xs:documentation>
					<xs:appinfo>
						<label>Tokenizer</label>
						<subclassOf>http://w3id.org/meta-share/omtd-share/Segmenter</subclassOf>
					</xs:appinfo>
				</xs:annotation>
			</xs:enumeration>
			<xs:enumeration value="http://w3id.org/meta-share/omtd-share/SentenceSplitter">
				<xs:annotation>
					<xs:documentation>A component that splits a text into sentences</xs:documentation>
					<xs:appinfo>
						<label>Sentence splitter</label>
						<subclassOf>http://w3id.org/meta-share/omtd-share/Segmenter</subclassOf>
					</xs:appinfo>
				</xs:annotation>
			</xs:enumeration>
			<xs:enumeration value="http://w3id.org/meta-share/omtd-share/Generator">
				<xs:annotation>
					<xs:documentation>A component that generates (semi-)automatically natural language texts (based on non-linguistic data, keywords, logical forms, knowledge bases...)</xs:documentation>
					<xs:appinfo>
						<label>Generator</label>
						<subclassOf>http://w3id.org/meta-share/omtd-share/Processor</subclassOf>
					</xs:appinfo>
				</xs:annotation>
			</xs:enumeration>
			<xs:enumeration value="http://w3id.org/meta-share/omtd-share/Summarizer">
				<xs:annotation>
					<xs:documentation>A component that produces a natural language synopsis of a longer text</xs:documentation>
					<xs:appinfo>
						<label>Summarizer</label>
						<subclassOf>http://w3id.org/meta-share/omtd-share/Generator</subclassOf>
					</xs:appinfo>
				</xs:annotation>
			</xs:enumeration>
			<xs:enumeration value="http://w3id.org/meta-share/omtd-share/Simplifier">
				<xs:annotation>
					<xs:documentation>A component that outputs a simpler rendition of a given item (sentence, text etc.)</xs:documentation>
					<xs:appinfo>
						<label>Simplifier</label>
						<subclassOf>http://w3id.org/meta-share/omtd-share/Generator</subclassOf>
					</xs:appinfo>
				</xs:annotation>
			</xs:enumeration>
			<xs:enumeration value="http://w3id.org/meta-share/omtd-share/PreOrPostProcessor">
				<xs:annotation>
					<xs:documentation>A component that is used at pre- or post-processing stages in order to normalize input/output</xs:documentation>
					<xs:appinfo>
						<label>Pre- or Post-Processor</label>
						<subclassOf>http://w3id.org/meta-share/omtd-share/ComponentType</subclassOf>
					</xs:appinfo>
				</xs:annotation>
			</xs:enumeration>
			<xs:enumeration value="http://w3id.org/meta-share/omtd-share/GrammarChecker">
				<xs:annotation>
					<xs:documentation>A component that corrects grammatical mistakes in a text</xs:documentation>
					<xs:appinfo>
						<label>Grammar checker</label>
						<subclassOf>http://w3id.org/meta-share/omtd-share/PreOrPostProcessor</subclassOf>
					</xs:appinfo>
				</xs:annotation>
			</xs:enumeration>
			<xs:enumeration value="http://w3id.org/meta-share/omtd-share/SpellingChecker">
				<xs:annotation>
					<xs:documentation>A component that corrects spelling mistakes in a text</xs:documentation>
					<xs:appinfo>
						<label>Spelling checker</label>
						<subclassOf>http://w3id.org/meta-share/omtd-share/PreOrPostProcessor</subclassOf>
					</xs:appinfo>
				</xs:annotation>
			</xs:enumeration>
			<xs:enumeration value="http://w3id.org/meta-share/omtd-share/Filter">
				<xs:annotation>
					<xs:documentation>A component that is used for filtering text input or annotations based on specific criteria</xs:documentation>
					<xs:appinfo>
						<label>Filter</label>
						<subclassOf>http://w3id.org/meta-share/omtd-share/PreOrPostProcessor</subclassOf>
					</xs:appinfo>
				</xs:annotation>
			</xs:enumeration>
			<xs:enumeration value="http://w3id.org/meta-share/omtd-share/Normalizer">
				<xs:annotation>
					<xs:documentation>A component that removes unwanted material from text (e.g. quotation marks, hyphenations etc.) or performs edits so that specific items (tokens, dates,  etc.) are substituted/represented with normalized values</xs:documentation>
					<xs:appinfo>
						<label>Normalizer</label>
						<subclassOf>http://w3id.org/meta-share/omtd-share/PreOrPostProcessor</subclassOf>
					</xs:appinfo>
				</xs:annotation>
			</xs:enumeration>
			<xs:enumeration value="http://w3id.org/meta-share/omtd-share/Analyzer">
				<xs:annotation>
					<xs:documentation>A component that is used for analyzing an input text in order to extract specific features/information (e.g. word list), or to produce statements over the whole text (e.g. classify it by topic)</xs:documentation>
					<xs:appinfo>
						<label>Analyzer</label>
						<subclassOf>http://w3id.org/meta-share/omtd-share/ComponentType</subclassOf>
						<altLabel>Extractor</altLabel>
					</xs:appinfo>
				</xs:annotation>
			</xs:enumeration>
			<xs:enumeration value="http://w3id.org/meta-share/omtd-share/EmotionRecognizer">
				<xs:annotation>
					<xs:documentation>A component that tries to recognize and annotate emotions (e.g. fear, anger, happiness etc.) from text, video, audio and image</xs:documentation>
					<xs:appinfo>
						<label>Emotion recognizer</label>
						<subclassOf>http://w3id.org/meta-share/omtd-share/Analyzer</subclassOf>
						<altLabel>Emotion detector</altLabel>
					</xs:appinfo>
				</xs:annotation>
			</xs:enumeration>
			<xs:enumeration value="http://w3id.org/meta-share/omtd-share/LexiconExtractorFromLexica">
				<xs:annotation>
					<xs:documentation>A component that extracts specific lexical information contained in other lexica</xs:documentation>
					<xs:appinfo>
						<label>Lexicon extractor from lexica</label>
						<subclassOf>http://w3id.org/meta-share/omtd-share/Analyzer</subclassOf>
					</xs:appinfo>
				</xs:annotation>
			</xs:enumeration>
			<xs:enumeration value="http://w3id.org/meta-share/omtd-share/EventExtractor">
				<xs:annotation>
					<xs:documentation>A component that tries to extract information related to incidents referred to in a text</xs:documentation>
					<xs:appinfo>
						<label>Event extractor</label>
						<subclassOf>http://w3id.org/meta-share/omtd-share/Analyzer</subclassOf>
					</xs:appinfo>
				</xs:annotation>
			</xs:enumeration>
			<xs:enumeration value="http://w3id.org/meta-share/omtd-share/LanguageIdentifier">
				<xs:annotation>
					<xs:documentation>A component that identifies the language of a given text based on its contents</xs:documentation>
					<xs:appinfo>
						<label>Language identifier</label>
						<subclassOf>http://w3id.org/meta-share/omtd-share/Analyzer</subclassOf>
					</xs:appinfo>
				</xs:annotation>
			</xs:enumeration>
			<xs:enumeration value="http://w3id.org/meta-share/omtd-share/PersuasiveExpressionMiner">
				<xs:annotation>
					<xs:documentation>A component that tries to identify persuasive expressions in a given text</xs:documentation>
					<xs:appinfo>
						<label>Persuasive expression miner</label>
						<subclassOf>http://w3id.org/meta-share/omtd-share/Analyzer</subclassOf>
					</xs:appinfo>
				</xs:annotation>
			</xs:enumeration>
			<xs:enumeration value="http://w3id.org/meta-share/omtd-share/ContradictionDetector">
				<xs:annotation>
					<xs:documentation>A component that tries to automatically recognize elements that reveal contradiction in a text</xs:documentation>
					<xs:appinfo>
						<label>Contradiction detector</label>
						<subclassOf>http://w3id.org/meta-share/omtd-share/Analyzer</subclassOf>
					</xs:appinfo>
				</xs:annotation>
			</xs:enumeration>
			<xs:enumeration value="http://w3id.org/meta-share/omtd-share/TermExtractor">
				<xs:annotation>
					<xs:documentation>A component that tries to extract terms from a corpus</xs:documentation>
					<xs:appinfo>
						<label>Term extractor</label>
						<subclassOf>http://w3id.org/meta-share/omtd-share/Analyzer</subclassOf>
						<altLabel>Terminology extractor</altLabel>
					</xs:appinfo>
				</xs:annotation>
			</xs:enumeration>
			<xs:enumeration value="http://w3id.org/meta-share/omtd-share/TopicExtractor">
				<xs:annotation>
					<xs:documentation>A component that guesses the topic of a text</xs:documentation>
					<xs:appinfo>
						<label>Topic extractor</label>
						<subclassOf>http://w3id.org/meta-share/omtd-share/Analyzer</subclassOf>
					</xs:appinfo>
				</xs:annotation>
			</xs:enumeration>
			<xs:enumeration value="http://w3id.org/meta-share/omtd-share/KeywordExtractor">
				<xs:annotation>
					<xs:documentation>A component that tries to extract keywords from a given text</xs:documentation>
					<xs:appinfo>
						<label>Keyword extractor</label>
						<subclassOf>http://w3id.org/meta-share/omtd-share/Analyzer</subclassOf>
					</xs:appinfo>
				</xs:annotation>
			</xs:enumeration>
			<xs:enumeration value="http://w3id.org/meta-share/omtd-share/LexiconExtractorFromCorpora">
				<xs:annotation>
					<xs:documentation>A component that extracts lexical information from corpora in order to produce structured lexical resources</xs:documentation>
					<xs:appinfo>
						<label>Lexicon extractor from corpora</label>
						<subclassOf>http://w3id.org/meta-share/omtd-share/Analyzer</subclassOf>
					</xs:appinfo>
				</xs:annotation>
			</xs:enumeration>
			<xs:enumeration value="http://w3id.org/meta-share/omtd-share/VariablesDectector">
				<xs:annotation>
					<xs:documentation>A component that tries to identify variables (in social sciences) in a text</xs:documentation>
					<xs:appinfo>
						<label>Variables dectector</label>
						<subclassOf>http://w3id.org/meta-share/omtd-share/Analyzer</subclassOf>
					</xs:appinfo>
				</xs:annotation>
			</xs:enumeration>
			<xs:enumeration value="http://w3id.org/meta-share/omtd-share/Disambiguator">
				<xs:annotation>
					<xs:documentation>A component that is used to disambiguate between two or more ambiguous items</xs:documentation>
					<xs:appinfo>
						<label>Disambiguator</label>
						<subclassOf>http://w3id.org/meta-share/omtd-share/Analyzer</subclassOf>
					</xs:appinfo>
				</xs:annotation>
			</xs:enumeration>
			<xs:enumeration value="http://w3id.org/meta-share/omtd-share/WordSenseDisambiguator">
				<xs:annotation>
					<xs:documentation>A component that tries to identify which sense of a word (i.e. meaning) is used in a sentence, when the word has multiple meanings</xs:documentation>
					<xs:appinfo>
						<label>Word sense disambiguator</label>
						<subclassOf>http://w3id.org/meta-share/omtd-share/Disambiguator</subclassOf>
					</xs:appinfo>
				</xs:annotation>
			</xs:enumeration>
			<xs:enumeration value="http://w3id.org/meta-share/omtd-share/DocumentClassifier">
				<xs:annotation>
					<xs:documentation>A component that tries to classify a document into one or more categories</xs:documentation>
					<xs:appinfo>
						<label>Document classifier</label>
						<subclassOf>http://w3id.org/meta-share/omtd-share/Analyzer</subclassOf>
					</xs:appinfo>
				</xs:annotation>
			</xs:enumeration>
			<xs:enumeration value="http://w3id.org/meta-share/omtd-share/SentimentAnalyzer">
				<xs:annotation>
					<xs:documentation>A component that tries to identify sentences that express the author&#8217;s negative or positive feelings on something</xs:documentation>
					<xs:appinfo>
						<label>Sentiment analyzer</label>
						<subclassOf>http://w3id.org/meta-share/omtd-share/Analyzer</subclassOf>
						<altLabel>Opinion mining tool</altLabel>
					</xs:appinfo>
				</xs:annotation>
			</xs:enumeration>
			<xs:enumeration value="http://w3id.org/meta-share/omtd-share/InformationExtractor">
				<xs:annotation>
					<xs:documentation>A component that automatically extracts structured information from unstructured and/or semi-structured machine-readable documents</xs:documentation>
					<xs:appinfo>
						<label>Information extractor</label>
						<subclassOf>http://w3id.org/meta-share/omtd-share/Analyzer</subclassOf>
					</xs:appinfo>
				</xs:annotation>
			</xs:enumeration>
			<xs:enumeration value="http://w3id.org/meta-share/omtd-share/Annotation">
				<xs:annotation>
					<xs:documentation>A note by way of explanation or comment added to a text or diagram [OED, https://en.oxforddictionaries.com/definition/annotation]. Text or corpus annotation refers to the interpretative linguistic information grounded in a knowledge resource that is added manually or automatically to a text or corpus respectively.</xs:documentation>
					<xs:appinfo>
						<label>Annotation</label>
						<subclassOf>http://www.w3.org/2002/07/owl#Thing</subclassOf>
						<subclassOf>http://w3id.org/meta-share/omtd-share/Operation</subclassOf>
						<altLabel>Labelling</altLabel>
						<altLabel>Tagging</altLabel>
					</xs:appinfo>
				</xs:annotation>
			</xs:enumeration>
			<xs:enumeration value="http://w3id.org/meta-share/omtd-share/TextAnnotation">
				<xs:annotation>
					<xs:documentation>The process/task of adding annotations (notes or comments) to a text; in TDM, the annotations refer mainly to the interpretative linguistic information grounded in a knowledge resource that is added manually or automatically to a text</xs:documentation>
					<xs:appinfo>
						<label>Text annotation</label>
						<subclassOf>http://w3id.org/meta-share/omtd-share/Annotation</subclassOf>
						<altLabel>Linguistic annotation</altLabel>
					</xs:appinfo>
				</xs:annotation>
			</xs:enumeration>
			<xs:enumeration value="http://w3id.org/meta-share/omtd-share/Parsing">
				<xs:annotation>
					<xs:documentation>The task/process of recognizing and marking the syntactic structure of a text or text segment</xs:documentation>
					<xs:appinfo>
						<label>Parsing</label>
						<subclassOf>http://w3id.org/meta-share/omtd-share/TextAnnotation</subclassOf>
						<subclassOf>http://w3id.org/meta-share/omtd-share/LinguisticAnalysis</subclassOf>
						<altLabel>Syntactic analysis</altLabel>
						<altLabel>Syntactic parsing</altLabel>
						<altLabel>Syntactic annotation</altLabel>
					</xs:appinfo>
				</xs:annotation>
			</xs:enumeration>
			<xs:enumeration value="http://w3id.org/meta-share/omtd-share/DependencyParsing">
				<xs:annotation>
					<xs:documentation>The task/process of identifying and marking the grammatical structure of a sentence, establishing relationships between "head" words and words that modify those heads</xs:documentation>
					<xs:appinfo>
						<label>Dependency parsing</label>
						<subclassOf>http://w3id.org/meta-share/omtd-share/Parsing</subclassOf>
					</xs:appinfo>
				</xs:annotation>
			</xs:enumeration>
			<xs:enumeration value="http://w3id.org/meta-share/omtd-share/Chunking">
				<xs:annotation>
					<xs:documentation>The task/process of dividing a sentence into chunks (non-overlapping text segments consisting of a head and preceding function words and/or modifiers)</xs:documentation>
					<xs:appinfo>
						<label>Chunking</label>
						<subclassOf>http://w3id.org/meta-share/omtd-share/Parsing</subclassOf>
						<altLabel>Light parsing</altLabel>
						<altLabel>Shallow parsing</altLabel>
					</xs:appinfo>
				</xs:annotation>
			</xs:enumeration>
			<xs:enumeration value="http://w3id.org/meta-share/omtd-share/ConstituencyParsing">
				<xs:annotation>
					<xs:documentation>The task/process of identifying and marking constituents (phrases, governed by a head and including function words and/or modifiers ) in a text or text segment</xs:documentation>
					<xs:appinfo>
						<label>Constituency parsing</label>
						<subclassOf>http://w3id.org/meta-share/omtd-share/Parsing</subclassOf>
						<altLabel>Phrase parsing</altLabel>
					</xs:appinfo>
				</xs:annotation>
			</xs:enumeration>
			<xs:enumeration value="http://w3id.org/meta-share/omtd-share/MorphosyntacticTagging">
				<xs:annotation>
					<xs:documentation>The task/process of adding morphosyntactic tags to words in a text, i.e. part-of-speech and, optionally, morphological features per part-of-speech.</xs:documentation>
					<xs:appinfo>
						<label>Morphosyntactic tagging</label>
						<subclassOf>http://w3id.org/meta-share/omtd-share/TextAnnotation</subclassOf>
						<altLabel>Morphosyntactic annotation</altLabel>
					</xs:appinfo>
				</xs:annotation>
			</xs:enumeration>
			<xs:enumeration value="http://w3id.org/meta-share/omtd-share/PosTagging">
				<xs:annotation>
					<xs:documentation>The task/process of marking words with the part of speech (word category, e.g. noun, verb etc.) to which they belong</xs:documentation>
					<xs:appinfo>
						<label>PoS Tagging</label>
						<subclassOf>http://w3id.org/meta-share/omtd-share/MorphosyntacticTagging</subclassOf>
						<altLabel>Grammatical annotation</altLabel>
						<altLabel>Grammatical tagging</altLabel>
					</xs:appinfo>
				</xs:annotation>
			</xs:enumeration>
			<xs:enumeration value="http://w3id.org/meta-share/omtd-share/BelowPosTagging">
				<xs:annotation>
					<xs:documentation>The annotation of words with morphological information besides the part of speech and dependent upon it (e.g. for nouns: gender, number and case; for verbs: tense, number, person etc.)</xs:documentation>
					<xs:appinfo>
						<label>Below PoS Tagging</label>
						<subclassOf>http://w3id.org/meta-share/omtd-share/MorphosyntacticTagging</subclassOf>
						<subclassOf>http://w3id.org/meta-share/omtd-share/MorphologicalAnnotation</subclassOf>
						<altLabel>Annotation of morphological features</altLabel>
						<altLabel>B-PoS Tagging</altLabel>
					</xs:appinfo>
				</xs:annotation>
			</xs:enumeration>
			<xs:enumeration value="http://w3id.org/meta-share/omtd-share/DiscourseAnnotation">
				<xs:annotation>
					<xs:documentation>The task/process of adding annotations relevant to discourse, such as discourse structure, discourse markers etc.</xs:documentation>
					<xs:appinfo>
						<label>Discourse annotation</label>
						<subclassOf>http://w3id.org/meta-share/omtd-share/TextAnnotation</subclassOf>
					</xs:appinfo>
				</xs:annotation>
			</xs:enumeration>
			<xs:enumeration value="http://w3id.org/meta-share/omtd-share/Argumentation">
				<xs:annotation>
					<xs:documentation>The act or process of forming reasons and of drawing conclusions and applying them to a case in discussion [https://www.merriam-webster.com/dictionary/argumentation]</xs:documentation>
					<xs:appinfo>
						<label>Argumentation</label>
						<subclassOf>http://w3id.org/meta-share/omtd-share/DiscourseAnnotation</subclassOf>
					</xs:appinfo>
				</xs:annotation>
			</xs:enumeration>
			<xs:enumeration value="http://w3id.org/meta-share/omtd-share/Lemmatization">
				<xs:annotation>
					<xs:documentation>Lemmatisation  (or lemmatization) in linguistics is the process of grouping together the inflected forms of a word so they can be analysed as a single item, identified by the word's lemma, or dictionary form. [Wikipedia]</xs:documentation>
					<xs:appinfo>
						<label>Lemmatization</label>
						<subclassOf>http://w3id.org/meta-share/omtd-share/TextAnnotation</subclassOf>
						<altLabel>Lemmatisation</altLabel>
					</xs:appinfo>
				</xs:annotation>
			</xs:enumeration>
			<xs:enumeration value="http://w3id.org/meta-share/omtd-share/StructuralAnnotation">
				<xs:annotation>
					<xs:documentation>The task/process of segmenting a text and recognizing textual structural units (paragraphs, sentences, words etc.)</xs:documentation>
					<xs:appinfo>
						<label>Structural annotation</label>
						<subclassOf>http://w3id.org/meta-share/omtd-share/TextAnnotation</subclassOf>
						<altLabel>Segmentation</altLabel>
					</xs:appinfo>
				</xs:annotation>
			</xs:enumeration>
			<xs:enumeration value="http://w3id.org/meta-share/omtd-share/Tokenization">
				<xs:annotation>
					<xs:documentation>The task/process of recognizing and tagging tokens (words, punctuation marks, digits etc.) in a text</xs:documentation>
					<xs:appinfo>
						<label>Tokenization</label>
						<subclassOf>http://w3id.org/meta-share/omtd-share/StructuralAnnotation</subclassOf>
					</xs:appinfo>
				</xs:annotation>
			</xs:enumeration>
			<xs:enumeration value="http://w3id.org/meta-share/omtd-share/SentenceSplitting">
				<xs:annotation>
					<xs:documentation>The task/process of recognizing and tagging sentence boundaries in a text</xs:documentation>
					<xs:appinfo>
						<label>Sentence splitting</label>
						<subclassOf>http://w3id.org/meta-share/omtd-share/StructuralAnnotation</subclassOf>
						<altLabel>Segmentation into sentences</altLabel>
					</xs:appinfo>
				</xs:annotation>
			</xs:enumeration>
			<xs:enumeration value="http://w3id.org/meta-share/omtd-share/AnnotationOfDocumentStructure">
				<xs:annotation>
					<xs:documentation>The task/process of annotating the internal structure of a document (e.g. book chapters, sections in a journal article, title, preface, images/figures etc.)</xs:documentation>
					<xs:appinfo>
						<label>Annotation of document structure</label>
						<subclassOf>http://w3id.org/meta-share/omtd-share/StructuralAnnotation</subclassOf>
					</xs:appinfo>
				</xs:annotation>
			</xs:enumeration>
			<xs:enumeration value="http://w3id.org/meta-share/omtd-share/ParagraphSplitting">
				<xs:annotation>
					<xs:documentation>The task/process of segmenting a text into paragraphs and marking their boundaries</xs:documentation>
					<xs:appinfo>
						<label>Paragraph splitting</label>
						<subclassOf>http://w3id.org/meta-share/omtd-share/StructuralAnnotation</subclassOf>
						<altLabel>Paragraph segmentation</altLabel>
					</xs:appinfo>
				</xs:annotation>
			</xs:enumeration>
			<xs:enumeration value="http://w3id.org/meta-share/omtd-share/Stemming">
				<xs:annotation>
					<xs:documentation>The task/process of cutting off the ends of words (mainly inflectional affixes but sometimes also derivational affixes) aiming to relate words to a base form.</xs:documentation>
					<xs:appinfo>
						<label>Stemming</label>
						<subclassOf>http://w3id.org/meta-share/omtd-share/TextAnnotation</subclassOf>
					</xs:appinfo>
				</xs:annotation>
			</xs:enumeration>
			<xs:enumeration value="http://w3id.org/meta-share/omtd-share/MorphologicalAnnotation">
				<xs:annotation>
					<xs:documentation>The task/process of adding annotations pertaining to the morphological level of analysis (e.g. gender, number, person etc.)</xs:documentation>
					<xs:appinfo>
						<label>Morphological annotation</label>
						<subclassOf>http://w3id.org/meta-share/omtd-share/TextAnnotation</subclassOf>
					</xs:appinfo>
				</xs:annotation>
			</xs:enumeration>
			<xs:enumeration value="http://w3id.org/meta-share/omtd-share/AnnotationOfMultiWordUnits">
				<xs:annotation>
					<xs:documentation>The task/process of annotating multi-word units, i.e. combinations of words that are considered as one</xs:documentation>
					<xs:appinfo>
						<label>Annotation of multi-word units</label>
						<subclassOf>http://w3id.org/meta-share/omtd-share/MorphologicalAnnotation</subclassOf>
					</xs:appinfo>
				</xs:annotation>
			</xs:enumeration>
			<xs:enumeration value="http://w3id.org/meta-share/omtd-share/AnnotationOfDerivationalFeatures">
				<xs:annotation>
					<xs:documentation>The task/process of adding annotations relevant to the derivational level of analysis (e.g. recognizing derivational affixes, tagging their meaning etc.)</xs:documentation>
					<xs:appinfo>
						<label>Annotation of derivational features</label>
						<subclassOf>http://w3id.org/meta-share/omtd-share/MorphologicalAnnotation</subclassOf>
					</xs:appinfo>
				</xs:annotation>
			</xs:enumeration>
			<xs:enumeration value="http://w3id.org/meta-share/omtd-share/AnnotationOfCompoundingFeatures">
				<xs:annotation>
					<xs:documentation>The task/process of marking compounds (single words composed of two or more free morphemes) and their parts</xs:documentation>
					<xs:appinfo>
						<label>Annotation of compounds</label>
						<subclassOf>http://w3id.org/meta-share/omtd-share/MorphologicalAnnotation</subclassOf>
					</xs:appinfo>
				</xs:annotation>
			</xs:enumeration>
			<xs:enumeration value="http://w3id.org/meta-share/omtd-share/WordSegmentation">
				<xs:annotation>
					<xs:documentation>The task/process of segmenting (cutting) a word into root and affixes</xs:documentation>
					<xs:appinfo>
						<label>Word segmentation</label>
						<subclassOf>http://w3id.org/meta-share/omtd-share/MorphologicalAnnotation</subclassOf>
					</xs:appinfo>
				</xs:annotation>
			</xs:enumeration>
			<xs:enumeration value="http://w3id.org/meta-share/omtd-share/BelowPosTagging">
				<xs:annotation>
					<xs:documentation>The annotation of words with morphological information besides the part of speech and dependent upon it (e.g. for nouns: gender, number and case; for verbs: tense, number, person etc.)</xs:documentation>
					<xs:appinfo>
						<label>Below PoS Tagging</label>
						<subclassOf>http://w3id.org/meta-share/omtd-share/MorphosyntacticTagging</subclassOf>
						<subclassOf>http://w3id.org/meta-share/omtd-share/MorphologicalAnnotation</subclassOf>
						<altLabel>Annotation of morphological features</altLabel>
						<altLabel>B-PoS Tagging</altLabel>
					</xs:appinfo>
				</xs:annotation>
			</xs:enumeration>
			<xs:enumeration value="http://w3id.org/meta-share/omtd-share/KnowledgeRepresentation">
				<xs:annotation>
					<xs:documentation>The task/process of representing information about entities in a form that machines are capable of understanding it</xs:documentation>
					<xs:appinfo>
						<label>Knowledge Representation</label>
						<subclassOf>http://w3id.org/meta-share/omtd-share/TextAndDataMining</subclassOf>
					</xs:appinfo>
				</xs:annotation>
			</xs:enumeration>
			<xs:enumeration value="http://w3id.org/meta-share/omtd-share/KnowledgeAcquisition">
				<xs:annotation>
					<xs:documentation>The process/task of extracting, organising and systematising knowledge usually of a specific domain from external sources so that it can be used in a knowledge-based system</xs:documentation>
					<xs:appinfo>
						<label>Knowledge acquisition</label>
						<subclassOf>http://w3id.org/meta-share/omtd-share/TextAndDataMining</subclassOf>
					</xs:appinfo>
				</xs:annotation>
			</xs:enumeration>
			<xs:enumeration value="http://w3id.org/meta-share/omtd-share/LexiconExtractionFromLexica">
				<xs:annotation>
					<xs:documentation>The task/process of constructing lexical resources based on the restructuring of lexical information contained in lexica (e.g. by parsing definitions or using syntactic information attached to lemmas)</xs:documentation>
					<xs:appinfo>
						<label>Lexicon extraction from lexica</label>
						<subclassOf>http://w3id.org/meta-share/omtd-share/SupportOperationForKnowledgeResources</subclassOf>
						<subclassOf>http://w3id.org/meta-share/omtd-share/KnowledgeAcquisition</subclassOf>
					</xs:appinfo>
				</xs:annotation>
			</xs:enumeration>
			<xs:enumeration value="http://w3id.org/meta-share/omtd-share/OntologyAcquisition">
				<xs:annotation>
					<xs:documentation>The task/process of creating an ontology based on other resources (corpora, other lexical resources, etc.)</xs:documentation>
					<xs:appinfo>
						<label>Ontology acquisition</label>
						<subclassOf>http://w3id.org/meta-share/omtd-share/KnowledgeAcquisition</subclassOf>
					</xs:appinfo>
				</xs:annotation>
			</xs:enumeration>
			<xs:enumeration value="http://w3id.org/meta-share/omtd-share/LexiconAcquisitionFromCorpora">
				<xs:annotation>
					<xs:documentation>The task/process of constructing lexical resources from corpora</xs:documentation>
					<xs:appinfo>
						<label>Lexicon acquisition from corpora</label>
						<subclassOf>http://w3id.org/meta-share/omtd-share/SupportOperationForKnowledgeResources</subclassOf>
						<subclassOf>http://w3id.org/meta-share/omtd-share/KnowledgeAcquisition</subclassOf>
					</xs:appinfo>
				</xs:annotation>
			</xs:enumeration>
			<xs:enumeration value="http://w3id.org/meta-share/omtd-share/OntologyEnhancement">
				<xs:annotation>
					<xs:documentation>The task/process of improving an ontology, typically by adding new relations or entities</xs:documentation>
					<xs:appinfo>
						<label>Ontology enhancement</label>
						<subclassOf>http://w3id.org/meta-share/omtd-share/KnowledgeAcquisition</subclassOf>
					</xs:appinfo>
				</xs:annotation>
			</xs:enumeration>
			<xs:enumeration value="http://w3id.org/meta-share/omtd-share/LexiconEnhancement">
				<xs:annotation>
					<xs:documentation>The task/process of improving (e.g. increasing the size of entries, improving the information, adding new types of information, etc.) a lexicon</xs:documentation>
					<xs:appinfo>
						<label>Lexicon enhancement</label>
						<subclassOf>http://w3id.org/meta-share/omtd-share/KnowledgeAcquisition</subclassOf>
						<subclassOf>http://w3id.org/meta-share/omtd-share/SupportOperationForKnowledgeResources</subclassOf>
					</xs:appinfo>
				</xs:annotation>
			</xs:enumeration>
			<xs:enumeration value="http://w3id.org/meta-share/omtd-share/BilingualLexiconInduction">
				<xs:annotation>
					<xs:documentation>The task/process of inducing word translations from monolingual or comparable corpora in two languages</xs:documentation>
					<xs:appinfo>
						<label>Bilingual lexicon induction</label>
						<subclassOf>http://w3id.org/meta-share/omtd-share/SupportOperationForKnowledgeResources</subclassOf>
						<subclassOf>http://w3id.org/meta-share/omtd-share/KnowledgeAcquisition</subclassOf>
					</xs:appinfo>
				</xs:annotation>
			</xs:enumeration>
			<xs:enumeration value="http://w3id.org/meta-share/omtd-share/InformationRetrieval">
				<xs:annotation>
					<xs:documentation>The activity of obtaining information resources relevant to an information need from a collection of information resources; searches can be based on full-text or other content-based indexing</xs:documentation>
					<xs:appinfo>
						<label>Information retrieval</label>
						<subclassOf>http://w3id.org/meta-share/omtd-share/TextAndDataMining</subclassOf>
					</xs:appinfo>
				</xs:annotation>
			</xs:enumeration>
			<xs:enumeration value="http://w3id.org/meta-share/omtd-share/ExploratorySearch">
				<xs:annotation>
					<xs:documentation>A type of search which, in contrast to traditional lookup search, covers a broad class of activities, such as investigating, evaluating, comparing, and synthesizing</xs:documentation>
					<xs:appinfo>
						<label>Exploratory search</label>
						<subclassOf>http://w3id.org/meta-share/omtd-share/InformationRetrieval</subclassOf>
					</xs:appinfo>
				</xs:annotation>
			</xs:enumeration>
			<xs:enumeration value="http://w3id.org/meta-share/omtd-share/InformationFiltering">
				<xs:annotation>
					<xs:documentation>The process/task of removing (filtering out) redundant or unwanted information from an information stream using (semi)automated or computerized methods prior to presentation to a human user; the selection of the items is based on the correlation between the content of the items and the user&#8217;s preferences (content-based filtering) or the correlation between people with similar preferences (collaborative filtering)</xs:documentation>
					<xs:appinfo>
						<label>Information filtering</label>
						<subclassOf>http://w3id.org/meta-share/omtd-share/InformationRetrieval</subclassOf>
					</xs:appinfo>
				</xs:annotation>
			</xs:enumeration>
			<xs:enumeration value="http://w3id.org/meta-share/omtd-share/InformationFilteringByRecommenderSystems">
				<xs:annotation>
					<xs:documentation>The delivery of information in the form of suggestions by recommender systems; recommender systems seek to predict the "rating" or "preference" that a user would give to an item</xs:documentation>
					<xs:appinfo>
						<label>Information filtering by recommender systems</label>
						<subclassOf>http://w3id.org/meta-share/omtd-share/InformationFiltering</subclassOf>
					</xs:appinfo>
				</xs:annotation>
			</xs:enumeration>
			<xs:enumeration value="http://w3id.org/meta-share/omtd-share/SemanticSearch">
				<xs:annotation>
					<xs:documentation>A type of search that seeks to improve search accuracy by understanding the searcher's intent and the contextual meaning of terms as they appear in the searchable dataspace, whether on the Web or within a closed system, to generate more relevant results.</xs:documentation>
					<xs:appinfo>
						<label>Semantic search</label>
						<subclassOf>http://w3id.org/meta-share/omtd-share/InformationRetrieval</subclassOf>
					</xs:appinfo>
				</xs:annotation>
			</xs:enumeration>
			<xs:enumeration value="http://w3id.org/meta-share/omtd-share/KnowledgeDiscovery">
				<xs:annotation>
					<xs:documentation>The task/process of automatically searching large volumes of data for patterns that can be considered knowledge about the data</xs:documentation>
					<xs:appinfo>
						<label>Knowledge discovery</label>
						<subclassOf>http://w3id.org/meta-share/omtd-share/TextAndDataMining</subclassOf>
					</xs:appinfo>
				</xs:annotation>
			</xs:enumeration>
			<xs:enumeration value="http://w3id.org/meta-share/omtd-share/TextCategorization">
				<xs:annotation>
					<xs:documentation>The task/process of assigning documents into classes or categories</xs:documentation>
					<xs:appinfo>
						<label>Text categorization</label>
						<subclassOf>http://w3id.org/meta-share/omtd-share/KnowledgeDiscovery</subclassOf>
						<subclassOf>http://w3id.org/meta-share/omtd-share/InformationExtraction</subclassOf>
						<altLabel>Document categorization</altLabel>
						<altLabel>Text classification</altLabel>
						<altLabel>Document categorisation</altLabel>
						<altLabel>Text categorisation</altLabel>
						<altLabel>Document classification</altLabel>
					</xs:appinfo>
				</xs:annotation>
			</xs:enumeration>
			<xs:enumeration value="http://w3id.org/meta-share/omtd-share/SentimentAnalysis">
				<xs:annotation>
					<xs:documentation>The process/task of computationally identifying and categorizing opinions expressed in a piece of text, especially in order to determine whether the writer's attitude towards a particular topic, product, etc. is positive, negative, or neutral</xs:documentation>
					<xs:appinfo>
						<label>Sentiment analysis</label>
						<subclassOf>http://w3id.org/meta-share/omtd-share/KnowledgeDiscovery</subclassOf>
						<subclassOf>http://w3id.org/meta-share/omtd-share/InformationExtraction</subclassOf>
						<altLabel>Opinion extraction</altLabel>
						<altLabel>Opinion mining</altLabel>
					</xs:appinfo>
				</xs:annotation>
			</xs:enumeration>
			<xs:enumeration value="http://w3id.org/meta-share/omtd-share/RecognizingTextualEntailment">
				<xs:annotation>
					<xs:documentation>A task/process that intends to recognize for two text fragments whether the meaning of one text is entailed in that of the other, i.e. whether the truth of one text fragment follows from that of the other fragment.</xs:documentation>
					<xs:appinfo>
						<label>Recognizing Textual Entailment</label>
						<subclassOf>http://w3id.org/meta-share/omtd-share/InformationExtraction</subclassOf>
						<subclassOf>http://w3id.org/meta-share/omtd-share/KnowledgeDiscovery</subclassOf>
					</xs:appinfo>
				</xs:annotation>
			</xs:enumeration>
			<xs:enumeration value="http://w3id.org/meta-share/omtd-share/TopicDetection">
				<xs:annotation>
					<xs:documentation>The task/process of identifying the topic of a text or dataset (e.g. by clustering keywords or using topic models)</xs:documentation>
					<xs:appinfo>
						<label>Topic detection</label>
						<subclassOf>http://w3id.org/meta-share/omtd-share/InformationExtraction</subclassOf>
						<subclassOf>http://w3id.org/meta-share/omtd-share/KnowledgeDiscovery</subclassOf>
						<altLabel>Topic extraction</altLabel>
					</xs:appinfo>
				</xs:annotation>
			</xs:enumeration>
			<xs:enumeration value="http://w3id.org/meta-share/omtd-share/QuestionAnswering">
				<xs:annotation>
					<xs:documentation>The task/process where computer systems try to automatically answer questions posed by users in the form of natural language.</xs:documentation>
					<xs:appinfo>
						<label>Question Answering</label>
						<subclassOf>http://w3id.org/meta-share/omtd-share/InformationExtraction</subclassOf>
						<subclassOf>http://w3id.org/meta-share/omtd-share/KnowledgeDiscovery</subclassOf>
					</xs:appinfo>
				</xs:annotation>
			</xs:enumeration>
			<xs:enumeration value="http://w3id.org/meta-share/omtd-share/Prediction">
				<xs:annotation>
					<xs:documentation>In Machine Learning, it refers to the use of algorithms that learn from previous data in order to make predictions on data (by estimating probabilities from previous data)</xs:documentation>
					<xs:appinfo>
						<label>Prediction</label>
						<subclassOf>http://w3id.org/meta-share/omtd-share/TextAndDataMining</subclassOf>
					</xs:appinfo>
				</xs:annotation>
			</xs:enumeration>
			<xs:enumeration value="http://w3id.org/meta-share/omtd-share/InformationExtraction">
				<xs:annotation>
					<xs:documentation>The process/task of automatically extracting structured information from unstructured and/or semi-structured data</xs:documentation>
					<xs:appinfo>
						<label>Information extraction</label>
						<subclassOf>http://w3id.org/meta-share/omtd-share/TextAndDataMining</subclassOf>
					</xs:appinfo>
				</xs:annotation>
			</xs:enumeration>
			<xs:enumeration value="http://w3id.org/meta-share/omtd-share/NamedEntityRecognition">
				<xs:annotation>
					<xs:documentation>A subtask of information extraction that seeks to locate and classify named entities in text into pre-defined categories such as the names of persons, organizations, locations, expressions of times, quantities, monetary values, percentages, etc.</xs:documentation>
					<xs:appinfo>
						<label>Named Entity Recognition</label>
						<subclassOf>http://w3id.org/meta-share/omtd-share/InformationExtraction</subclassOf>
					</xs:appinfo>
				</xs:annotation>
			</xs:enumeration>
			<xs:enumeration value="http://w3id.org/meta-share/omtd-share/RecognizingTextualEntailment">
				<xs:annotation>
					<xs:documentation>A task/process that intends to recognize for two text fragments whether the meaning of one text is entailed in that of the other, i.e. whether the truth of one text fragment follows from that of the other fragment.</xs:documentation>
					<xs:appinfo>
						<label>Recognizing Textual Entailment</label>
						<subclassOf>http://w3id.org/meta-share/omtd-share/InformationExtraction</subclassOf>
						<subclassOf>http://w3id.org/meta-share/omtd-share/KnowledgeDiscovery</subclassOf>
					</xs:appinfo>
				</xs:annotation>
			</xs:enumeration>
			<xs:enumeration value="http://w3id.org/meta-share/omtd-share/PersuasiveExpressionMining">
				<xs:annotation>
					<xs:documentation>The task/process of identifying and extracting (especially from political speech texts) pieces of text that aim to persuade</xs:documentation>
					<xs:appinfo>
						<label>Persuasive expression mining</label>
						<subclassOf>http://w3id.org/meta-share/omtd-share/InformationExtraction</subclassOf>
					</xs:appinfo>
				</xs:annotation>
			</xs:enumeration>
			<xs:enumeration value="http://w3id.org/meta-share/omtd-share/ComputationalArgumentation">
				<xs:annotation>
					<xs:documentation>The process/task of identifying and representing argumentation in text, so that systems have the ability to use them in tasks, such as automated logical reasoning</xs:documentation>
					<xs:appinfo>
						<label>Computational argumentation</label>
						<subclassOf>http://w3id.org/meta-share/omtd-share/InformationExtraction</subclassOf>
					</xs:appinfo>
				</xs:annotation>
			</xs:enumeration>
			<xs:enumeration value="http://w3id.org/meta-share/omtd-share/ExtractionOfDomainSpecificInformation">
				<xs:annotation>
					<xs:documentation>Extraction of information that pertains to specific domains/disciplines; it can be used combined with "Annotation type" to specify the type of information extracted</xs:documentation>
					<xs:appinfo>
						<label>Extraction of domain-specific information</label>
						<subclassOf>http://w3id.org/meta-share/omtd-share/InformationExtraction</subclassOf>
					</xs:appinfo>
				</xs:annotation>
			</xs:enumeration>
			<xs:enumeration value="http://w3id.org/meta-share/omtd-share/ExtractionOfFundingInformation">
				<xs:annotation>
					<xs:documentation>The task/process of detecting in a text and extracting information relevant to funding (e.g. funding programme, award, funder etc.)</xs:documentation>
					<xs:appinfo>
						<label>Extraction of funding information</label>
						<subclassOf>http://w3id.org/meta-share/omtd-share/ExtractionOfDomainSpecificInformation</subclassOf>
						<altLabel>Mining of funding information</altLabel>
					</xs:appinfo>
				</xs:annotation>
			</xs:enumeration>
			<xs:enumeration value="http://w3id.org/meta-share/omtd-share/ContradictionDetection">
				<xs:annotation>
					<xs:documentation>The task/process of identifying conflicting statements (contradictions) in a dataset</xs:documentation>
					<xs:appinfo>
						<label>Contradiction detection</label>
						<subclassOf>http://w3id.org/meta-share/omtd-share/InformationExtraction</subclassOf>
					</xs:appinfo>
				</xs:annotation>
			</xs:enumeration>
			<xs:enumeration value="http://w3id.org/meta-share/omtd-share/KeywordExtraction">
				<xs:annotation>
					<xs:documentation>The task/process of identifying keywords (words deemed indicative of the topic/subject) in a text/corpus</xs:documentation>
					<xs:appinfo>
						<label>Keyword extraction</label>
						<subclassOf>http://w3id.org/meta-share/omtd-share/InformationExtraction</subclassOf>
					</xs:appinfo>
				</xs:annotation>
			</xs:enumeration>
			<xs:enumeration value="http://w3id.org/meta-share/omtd-share/RelationExtraction">
				<xs:annotation>
					<xs:documentation>The process/task of identifying and classifying relation mentions between entities in text and/or data.</xs:documentation>
					<xs:appinfo>
						<label>Relation extraction</label>
						<subclassOf>http://w3id.org/meta-share/omtd-share/InformationExtraction</subclassOf>
					</xs:appinfo>
				</xs:annotation>
			</xs:enumeration>
			<xs:enumeration value="http://w3id.org/meta-share/omtd-share/TopicDetection">
				<xs:annotation>
					<xs:documentation>The task/process of identifying the topic of a text or dataset (e.g. by clustering keywords or using topic models)</xs:documentation>
					<xs:appinfo>
						<label>Topic detection</label>
						<subclassOf>http://w3id.org/meta-share/omtd-share/InformationExtraction</subclassOf>
						<subclassOf>http://w3id.org/meta-share/omtd-share/KnowledgeDiscovery</subclassOf>
						<altLabel>Topic extraction</altLabel>
					</xs:appinfo>
				</xs:annotation>
			</xs:enumeration>
			<xs:enumeration value="http://w3id.org/meta-share/omtd-share/TermExtraction">
				<xs:annotation>
					<xs:documentation>The act/process of identifying and extracting candidate terms from a domain-specific corpus</xs:documentation>
					<xs:appinfo>
						<label>Term extraction</label>
						<subclassOf>http://w3id.org/meta-share/omtd-share/InformationExtraction</subclassOf>
						<altLabel>Terminology extraction</altLabel>
					</xs:appinfo>
				</xs:annotation>
			</xs:enumeration>
			<xs:enumeration value="http://w3id.org/meta-share/omtd-share/TextCategorization">
				<xs:annotation>
					<xs:documentation>The task/process of assigning documents into classes or categories</xs:documentation>
					<xs:appinfo>
						<label>Text categorization</label>
						<subclassOf>http://w3id.org/meta-share/omtd-share/KnowledgeDiscovery</subclassOf>
						<subclassOf>http://w3id.org/meta-share/omtd-share/InformationExtraction</subclassOf>
						<altLabel>Document categorization</altLabel>
						<altLabel>Text classification</altLabel>
						<altLabel>Document categorisation</altLabel>
						<altLabel>Text categorisation</altLabel>
						<altLabel>Document classification</altLabel>
					</xs:appinfo>
				</xs:annotation>
			</xs:enumeration>
			<xs:enumeration value="http://w3id.org/meta-share/omtd-share/EmotionDetection">
				<xs:annotation>
					<xs:documentation>The process/task of identifying types of feelings (e.g. anger, fear, happiness, sadness, etc.) in the linguistic expression of texts or facial expressions</xs:documentation>
					<xs:appinfo>
						<label>Emotion detection</label>
						<subclassOf>http://w3id.org/meta-share/omtd-share/InformationExtraction</subclassOf>
						<altLabel>Emotion recognition</altLabel>
					</xs:appinfo>
				</xs:annotation>
			</xs:enumeration>
			<xs:enumeration value="http://w3id.org/meta-share/omtd-share/EntityMentionRecognition">
				<xs:annotation>
					<xs:documentation>The task/process of detecting in a text mentions of a specific class of entities (e.g. biochemical entities, historical persons)</xs:documentation>
					<xs:appinfo>
						<label>Entity mention recognition</label>
						<subclassOf>http://w3id.org/meta-share/omtd-share/InformationExtraction</subclassOf>
					</xs:appinfo>
				</xs:annotation>
			</xs:enumeration>
			<xs:enumeration value="http://w3id.org/meta-share/omtd-share/QuestionAnswering">
				<xs:annotation>
					<xs:documentation>The task/process where computer systems try to automatically answer questions posed by users in the form of natural language.</xs:documentation>
					<xs:appinfo>
						<label>Question Answering</label>
						<subclassOf>http://w3id.org/meta-share/omtd-share/InformationExtraction</subclassOf>
						<subclassOf>http://w3id.org/meta-share/omtd-share/KnowledgeDiscovery</subclassOf>
					</xs:appinfo>
				</xs:annotation>
			</xs:enumeration>
			<xs:enumeration value="http://w3id.org/meta-share/omtd-share/TemporalExpressionRecognition">
				<xs:annotation>
					<xs:documentation>The task/process of identifying temporal expressions (also called timex) in a text in order to extract temporal information</xs:documentation>
					<xs:appinfo>
						<label>Temporal expression recognition</label>
						<subclassOf>http://w3id.org/meta-share/omtd-share/InformationExtraction</subclassOf>
					</xs:appinfo>
				</xs:annotation>
			</xs:enumeration>
			<xs:enumeration value="http://w3id.org/meta-share/omtd-share/SentimentAnalysis">
				<xs:annotation>
					<xs:documentation>The process/task of computationally identifying and categorizing opinions expressed in a piece of text, especially in order to determine whether the writer's attitude towards a particular topic, product, etc. is positive, negative, or neutral</xs:documentation>
					<xs:appinfo>
						<label>Sentiment analysis</label>
						<subclassOf>http://w3id.org/meta-share/omtd-share/KnowledgeDiscovery</subclassOf>
						<subclassOf>http://w3id.org/meta-share/omtd-share/InformationExtraction</subclassOf>
						<altLabel>Opinion extraction</altLabel>
						<altLabel>Opinion mining</altLabel>
					</xs:appinfo>
				</xs:annotation>
			</xs:enumeration>
			<xs:enumeration value="http://w3id.org/meta-share/omtd-share/EventDetection">
				<xs:annotation>
					<xs:documentation>The process/task of identifying events in data (text, video, images etc.), usually combined with their classification into types of events and recognition of the event attributes (e.g. time, place, participants and duration)</xs:documentation>
					<xs:appinfo>
						<label>Event detection</label>
						<subclassOf>http://w3id.org/meta-share/omtd-share/InformationExtraction</subclassOf>
						<altLabel>Event extraction</altLabel>
					</xs:appinfo>
				</xs:annotation>
			</xs:enumeration>
		</xs:restriction>
	</xs:simpleType>
	<xs:simpleType name="dataFormatType">
		<xs:annotation>
			<xs:documentation>Specifies the format that is used since often the mime type will not be sufficient for machine processing; NOTE: normally the format should be represented as a combination of the mimetype (e.g. application/xml) and some name and link to the documentation about the supplementary conventions used (e.g xces, alvisED etc.)</xs:documentation>
			<xs:appinfo>
				<xs:label>Data format</xs:label>
				<usage-ugc>Automatically filled in with the dataFormat of the publications</usage-ugc>
			</xs:appinfo>
		</xs:annotation>
		<xs:restriction base="xs:anyURI">
			<xs:enumeration value="http://w3id.org/meta-share/omtd-share/Solr">
				<xs:annotation>
					<xs:documentation>Solr format</xs:documentation>
					<xs:appinfo>
						<label>Solr</label>
						<subclassOf>http://w3id.org/meta-share/omtd-share/DataFormat</subclassOf>
					</xs:appinfo>
				</xs:annotation>
			</xs:enumeration>
			<xs:enumeration value="http://w3id.org/meta-share/omtd-share/TabularFormat">
				<xs:annotation>
					<xs:documentation>Any format based on columns</xs:documentation>
					<xs:appinfo>
						<label>Tabular format</label>
						<subclassOf>http://w3id.org/meta-share/omtd-share/DataFormat</subclassOf>
					</xs:appinfo>
				</xs:annotation>
